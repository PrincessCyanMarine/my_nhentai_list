{
    "manifest_version": 3,
    "name": "My Hentai List",
    "description": "An extension that allows rating and saving nhentai doujinshi to a list.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "unlimitedStorage"
    ],
    "key": "",
    "content_scripts": [
        {
            "js": [
                "scripts/injector.js"
            ],
            "matches": [
                "https://nhentai.net/g/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "scripts/infoGetter.js"
            ],
            "matches": [
                "https://nhentai.net/g/*"
            ],
            "run_at": "document_end",
            "world": "MAIN"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/assets/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "action": {
        "default_title": "See saved doujinshi",
        "default_popup": "popup.html"
    }
}